<mj-section background-color="#fff" padding-top="0" padding-bottom="5px">
  {% for user in users %}
  <mj-column width="25%" padding-bottom="16px">

    <mj-image width="80px" height="80px" rechat-assets="avatar" src="{{ user.profile_image_url }}" padding-bottom="8px"></mj-image>

    <mj-text rechat-editable="true" font-family="Helvetica Neue,Helvetica,system-ui,-apple-system,BlinkMacSystemFont,Segoe UI,Arial,sans-serif" font-weight="500" font-size="14px" align="center" color="#262626">
      {{user.display_name}}
    </mj-text>

  </mj-column>
  {% endfor %}
</mj-section>
