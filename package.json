  "scripts": {
    "develop": "NODE_ENV=development nodemon ./bin/server --config ./nodemon.config.json --exec babel-node",
    "developClient": "NODE_ENV=development notify=true babel-node ./bin/server webpack-dev-server --progress --hot --host=192.168.1.102",
    "build": "rimraf dist && NODE_ENV=production babel-node ./build",
    "build:stage": "rimraf dist && NODE_ENV=stage babel-node ./build",
    "serve": "NODE_ENV=production forever node_modules/babel-cli/bin/babel-node.js ./bin/server",
    "serve:stage": "NODE_ENV=stage babel-node ./bin/server",
    "lint": "npm run lint:app && npm run lint:server",
    "lint:app": "eslint ./app",
    "lint:server": "eslint ./server",
    "test": "npm run test:app",
    "test:app": "jest -u --setupTestFrameworkScriptFile ./setupTests.js",
    "test:coverage": "npm run test:app -- --coverage",
    "test:watch": "npm test -- --watch",
    "test:e2e": "nightwatch",
    "e2e-setup": "selenium-standalone install",
    "heroku-postbuild": "npm run build",
    "postinstall": "node ./npm-postinstall.js",
    "storybook": "start-storybook -p 6006 -c stories",
    "storybook:build": "build-storybook -c stories -o .tmp/stories",
    "storybook:publish": "npm run storybook:build && surge -p .tmp/stories -d rechat-stories.surge.sh"
