  "scripts": {
    "dev": "NODE_ENV=development nodemon ./bin/server --ignore ./app --exec babel-node",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "NODE_ENV=development mocha --compilers js:babel/register",
    "test:models-alert": "NODE_ENV=development mocha test/models-alert --compilers js:babel/register",
    "test:models-listing": "NODE_ENV=development mocha test/models-listing --compilers js:babel/register",
    "test:models-room": "NODE_ENV=development mocha test/models-room --compilers js:babel/register",
    "test:models-task": "NODE_ENV=development mocha test/models-task --compilers js:babel/register",
    "test:models-transaction": "NODE_ENV=development mocha test/models-transaction --compilers js:babel/register",
    "test:models-user": "NODE_ENV=development mocha test/models-user --compilers js:babel/register",
    "test:models-rec": "NODE_ENV=development mocha test/models-rec --compilers js:babel/register",
    "test:models-branch": "NODE_ENV=development mocha test/models-branch --compilers js:babel/register",
    "test:models-user-shadow": "NODE_ENV=development mocha test/models-user-shadow --compilers js:babel/register",
    "test:models-brand": "NODE_ENV=development mocha test/models-brand --compilers js:babel/register",
    "test:models-school": "NODE_ENV=development mocha test/models-school --compilers js:babel/register",
    "start": "npm run production",
    "api": "NODE_ENV=development DEV_PORT=8080 nodemon app/app.js",
    "webpack-dev-server": "WEBPACK_PROCESS=build NODE_ENV=development PORT=8080 webpack-dev-server --content-base app/public/ --hot --inline --devtool inline-source-map --history-api-fallback",
    "development": "cp app/views/index.html app/public/index.html && WEBPACK_PROCESS=build NODE_ENV=development webpack && npm run webpack-dev-server",
    "staging": "rm -rf app/public/index.html && WEBPACK_PROCESS=build NODE_ENV=staging webpack -p --devtool inline-source-map && NODE_ENV=staging node app/app.js",
    "production": "rm -rf app/public/index.html && WEBPACK_PROCESS=build NODE_ENV=production webpack -p && NODE_ENV=production node app/app.js"
  },
