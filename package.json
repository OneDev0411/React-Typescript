{
  "name": "rechat",
  "version": "61.0.2",
  "main": "app.js",
  "repository": "git@gitlab.com:rechat/web.git",
  "scripts": {
    "commit": "commit",
    "develop": "NODE_ENV=development nodemon ./bin/server --config ./nodemon.config.json --exec babel-node --extensions \".js,.ts,.tsx\"",
    "developClient": "NODE_ENV=development notify=true npx babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "debug": "ndb npm run develop",
    "icons": "babel-node ./icon-manager",
    "build": "rimraf dist && NODE_ENV=production babel-node ./build.js",
    "build:stage": "rimraf dist && NODE_ENV=stage babel-node ./build.js",
    "build:ci": "NODE_ENV=ci babel-node ./build.js",
    "serve": "NODE_ENV=production forever node_modules/@babel/node/bin/babel-node.js --extensions \".js,.ts,.tsx\" ./bin/server ",
    "serve:ci": "NODE_ENV=ci PORT=8081 babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "serve:stage": "NODE_ENV=stage babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "styled-svg": "styled-svg app/views/components/SvgIcons/**/*.svg --no-tests --size=24x24",
    "types:check": "tsc",
    "lint": "npm run lint:app && npm run lint:server && npm run lint:tests",
    "lint:staged": "eslint `git diff --diff-filter=d --staged --name-only`",
    "lint:fix-staged": "npm run lint:staged -- --fix",
    "lint:app": "eslint ./app",
    "lint:server": "eslint ./server",
    "lint:tests": "eslint ./tests --fix",
    "e2e:start": "npm run e2e:api:update-image && npm run e2e:api:start & npm run e2e:server:start",
    "e2e:server:start": "NODE_ENV=development E2E=true PORT=8081 notify=true npx babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "e2e:api:start": "cd tests/services && docker-compose down && docker-compose build --no-cache && docker-compose up",
    "e2e:api:update-image": "cd tests/services && docker-compose pull api",
    "e2e:api:fetch-config": "heroku config -s --app rechat-boer | sed -E \"s/(.*)='(.*)'/\\1=\\2/\" > tests/services/api/.env",
    "e2e:update-db": "docker exec e2e_postgres pg_dump --clean --if-exists -U rechat rechat > tests/services/db/data/test-data.sql",
    "test:e2e": "cypress run --browser chrome",
    "test:unit": "NODE_ENV=ci jest",
    "test:update-snapshots": "npm run test:unit -- -u",
    "test:ci": "npm run test:unit && npm run test:e2e",
    "test": "jest --watch",
    "jest": "jest",
    "cypress": "cypress open"
  },
