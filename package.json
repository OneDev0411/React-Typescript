{
  "name": "rechat",
  "version": "79.0.1",
  "main": "app.js",
  "repository": "git@gitlab.com:rechat/web.git",
  "scripts": {
    "commit": "commit",
    "develop": "cross-env NODE_ENV=development NODE_OPTIONS='--max_old_space_size=4096' nodemon ./bin/server --config ./nodemon.config.json --exec babel-node --extensions \".js,.ts,.tsx\"",
    "developClient": "cross-env NODE_ENV=development notify=true babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "debug": "ndb npm run develop",
    "icons": "babel-node ./icon-manager",
    "build": "rimraf dist && cross-env NODE_ENV=production babel-node ./build.js && rimraf dist/*.js.map",
    "build:stage": "rimraf dist && cross-env NODE_ENV=stage babel-node ./build.js",
    "build:ci": "cross-env NODE_ENV=ci NODE_OPTIONS='--max-old-space-size=8192' babel-node ./build.js",
    "serve": "cross-env NODE_ENV=production forever node_modules/@babel/node/bin/babel-node.js --extensions \".js,.ts,.tsx\" ./bin/server ",
    "serve:ci": "cross-env NODE_ENV=ci PORT=8081 babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "serve:stage": "cross-env NODE_ENV=stage babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "styled-svg": "styled-svg app/views/components/SvgIcons/**/*.svg --no-tests --size=24x24",
    "types:check": "tsc",
    "lint": "npm run lint:app && npm run lint:server && npm run lint:tests",
    "lint:fix-staged": "lint-staged",
    "lint:app": "eslint ./app",
    "lint:server": "eslint ./server",
    "lint:tests": "eslint ./tests --fix",
    "e2e:start": "npm run e2e:api:update-image && npm run e2e:api:start & npm run e2e:server:start",
    "e2e:server:start": "cross-env NODE_ENV=development E2E=true PORT=8081 notify=true babel-node --extensions \".js,.ts,.tsx\" ./bin/server",
    "e2e:api:start": "cd tests/services && docker-compose down && docker-compose build --no-cache && docker-compose up",
    "e2e:api:update-image": "cd tests/services && docker-compose pull api",
    "e2e:api:fetch-config": "heroku config -s --app rechat-boer | sed -E \"s/(.*)='(.*)'/\\1=\\2/\" > tests/services/api/.env",
    "e2e:update-db": "docker exec e2e_postgres pg_dump --clean --if-exists -U rechat rechat > tests/services/db/data/test-data.sql",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:update-snapshots": "npm run test:unit -- -u",
    "test:ci": "npm run test:unit",
    "test": "jest --watch",
    "jest": "jest"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "none",
    "arrowParens": "avoid",
    "endOfLine": "auto"
  },
  "lint-staged": {
    "**/*.(js|ts|tsx|json|css|html|md)": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS && ./githooks/commit-msg.js $HUSKY_GIT_PARAMS `git rev-parse --abbrev-ref HEAD`"
    }
  },
